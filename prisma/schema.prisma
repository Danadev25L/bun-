generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Todo {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  tag         String?
  date        DateTime
  completed   Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
